generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_CONNECTION_STRING")
}

model UserSubmittedTuning {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  createdAt DateTime @default(now())
  tuningName String @unique
  friendlyName String?
  tags String[] @default([""])
  likes Int @default(0)
  tuning Note[]
}


type Note {
  note String 
  octave Int
}